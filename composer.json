{
    "name": "cleaniquecoders/profile",
    "description": "Common Profile Information",
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\Profile\\": "src/"
        },
        "classmap": [
            "database/factories",
            "database/migrations",
            "database/seeds"
        ],
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\Profile\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^5.5|^5.6|^5.7|^5.8",
        "illuminate/auth": "^5.5|^5.6|^5.7|^5.8",
        "cleaniquecoders/blueprint-macro": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0|^8.0",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*",
        "mockery/mockery": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\Profile\\ProfileServiceProvider"
            ]
        }
    }
}
